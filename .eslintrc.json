{
    "env": {
        "browser": true
    },
    "extends": ["airbnb-base", "prettier"],
    "plugins": ["prettier"],
    "parser": "@babel/eslint-parser",
    "rules": {
        "no-unused-vars": "warn",
        "no-console": "off",
        "func-names": "off",
        "no-plusplus": "off",
        "no-underscore-dangle": ["error", { "allowAfterThis": true }],
        "class-methods-use-this": "off",
        "prefer-arrow-callback": "off",
        "prettier/prettier": ["error", {
            "endOfLine":"auto"
        }]
    }
}
